{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": [
      "off",
      {
        "aria-required-children": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/name-role-value": "off",
    "highest-available-document-mode": "off",
    "axe/structure": "off",
    "axe/text-alternatives": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-touch-action"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "meta-viewport": "off",
    "apple-touch-icons": "off"
  }
}